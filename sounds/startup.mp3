}

}

function ZeigeLeiste(){

if (window.getComputedStyle(document.getElementById("quicktime")).display!="block" ){
	document.getElementById("quicktime").style.display="block";
} else {
	document.getElementById("quicktime").style.display="none";
}

}

function Zeige3DBox(){

